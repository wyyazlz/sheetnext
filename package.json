  "keywords": [
    "spreadsheet",
    "excel",
    "editor",
    "table",
    "ai",
    "sheetnext",
    "sheet",
    "spreadsheet editor",
    "ai excel",
    "ai spreadsheet"
  ],
  "dependencies": {
    "fast-xml-parser": "^5.3.0"
  },
  "devDependencies": {
    "vite": "^7.1.9"
  }
